function main() {
    printf("original\n");
    var v: Ptr[Vector[Int]] = Vector[Int].new(4);
    printf("length = %zu\n", v.length);
    (*v)[0] = 11;
    (*v)[1] = 23;
    (*v)[2] = 35;
    (*v)[3] = 47;
    // v = [1, 2, 3, 4];
    printf("first element = %i\n", (*v)[0]);
    printf("second element = %i\n", (*v)[1]);
    printf("third element = %i\n", (*v)[2]);
    printf("fourth element = %i\n", (*v)[3]);
    for x in *v {
        printf("iterating: %i\n", x);
    }

    printf("copy\n");
    var v2: Ptr[Vector[Int]] = v.copy();
    printf("length = %zu\n", v2.length);
    printf("first element = %i\n", (*v2)[0]);
    printf("third element = %i\n", (*v2)[2]);
    for x in *v2 {
        printf("%i\n", x);
    }
}
